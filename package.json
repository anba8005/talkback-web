{
	"private": true,
	"name": "talkback-web",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"build": "preact build --no-prerender --brotli",
		"serve": "sirv build --port 4000 --cors --single",
		"analyze": "source-map-explorer 'build/*.js'",
		"postversion": "git push origin v$npm_package_version && git push origin master",
		"dev": "preact watch --port 4000"
	},
	"eslintIgnore": [
		"build/*"
	],
	"browserslist": [
		"> 1%",
		"IE >= 9",
		"last 2 versions"
	],
	"dependencies": {
		"@material-ui/core": "^4.11.2",
		"@material-ui/icons": "^4.11.2",
		"clsx": "^1.1.1",
		"janus-gateway-js": "^2.0.2",
		"mobx": "^6.0.4",
		"mobx-react-lite": "^3.1.6",
		"mobx-sync": "^3.0.0",
		"preact": "^10.5.9",
		"preact-jsx-chai": "^2.3.2",
		"preact-markup": "^1.6.0",
		"preact-render-to-string": "^5.1.12",
		"strongly-typed-events": "^2.0.6"
	},
	"devDependencies": {
		"@teamsupercell/typings-for-css-modules-loader": "^2.4.0",
		"@types/webpack-env": "^1.16.0",
		"@typescript-eslint/eslint-plugin": "^4.12.0",
		"@typescript-eslint/parser": "^4.12.0",
		"css-loader": "^5.0.1",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.1",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-react": "^7.19.0",
		"eslint-plugin-react-hooks": "^3.0.0",
		"preact-cli": "^3.0.5",
		"prettier": "^2.2.1",
		"sirv-cli": "^1.0.10",
		"typescript": "^4.1.3"
	}
}
